	<script>
		$(function() {
			// 切换
			$(".list-inline li").click(
					function() {
						var cut = String($(this).find('a').prop('href')).split(
								'#')[1];
						$("div[id^=hcs]").hide();
						$("#" + cut).show();
						$(this).find('a').addClass('activex').end().siblings()
								.find('a').removeClass('activex');
					});
			$("#woman").css("display", "none");
			$(".man")
					.bind(
							"click",
							function() {
								$(".xb").css("display", "block");
								$(".xbnv").css("display", "none");
								$(".man")
										.css("background-image",
												"url('${oss}/resources/assets/images/guid/icon_03.jpg')");
								$(".woman")
										.css("background-image",
												"url('${oss}/resources/assets/images/guid/icon_06.jpg')");
								$("#man").css("display", "block");
								$("#woman").css("display", "none");
							});
			$(".woman")
					.bind(
							"click",
							function() {
								$("#man").css("display", "none");
								$("#woman").css("display", "block");
								$(".man")
										.css("background-image",
												"url('${oss}/resources/assets/images/guid/icon_04.jpg')");
								$(".woman")
										.css("background-image",
												"url('${oss}/resources/assets/images/guid/icon_05.jpg')");
								$(".xb").css("display", "none");
								$(".xbnv").css("display", "block");
							});
			// 理财攻略
			$('#t1').css('display', 'block');
			var tcen = $('.kx>div');
			tcen.bind("mouseover", function() {
				$(this).css('cursor', 'pointer');
				var sid = '#' + $(this).attr('class');
				if (sid == '#undefined') {
					return;
				}
				if (sid != '#t1') {
					$(sid).css({
						display : 'block'
					});
					$('#t1').css('display', 'none');
				}
			})
			tcen.bind("mouseout", function() {
				var xsid = '#' + $(this).attr('class');
				if (xsid == '#undefined' && xsid == '#t1') {
					return;
				}
				$(xsid).css('display', 'none');
				$('#t1').css('display', 'block');
			});
		});
	</script>
